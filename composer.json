{
   "name": "devzair/mail-logger-bundle",
   "description": "Symfony bundle to log all emails sent via Symfony Mailer",
   "type": "symfony-bundle",
   "license": "MIT",
   "authors": [
      {
         "name": "DevZair",
         "email": "contact@devzair.fr"
      }
   ],
   "require": {
      "php": ">=8.1",
      "symfony/mailer": "^6.0 || ^7.0",
      "symfony/mime": "^6.0 || ^7.0",
      "symfony/dependency-injection": "^6.0 || ^7.0",
      "symfony/config": "^6.0 || ^7.0",
      "symfony/http-kernel": "^6.0 || ^7.0",
      "doctrine/orm": "^2.14 || ^3.0",
      "symfony/validator": "^7.2"
   },
   "autoload": {
      "psr-4": {
         "DevZair\\MailLoggerBundle\\": "src/"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "DevZair\\MailLoggerBundle\\Tests\\": "tests/"
      }
   },
   "extra": {
      "branch-alias": {
         "dev-main": "1.0-dev"
      }
   },
   "require-dev": {
      "phpunit/phpunit": "^11.5"
   }
}

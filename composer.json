{
   "name": "devzair/mail-logger-bundle",
   "description": "Symfony bundle to log all emails sent via Symfony Mailer",
   "type": "symfony-bundle",
   "license": "MIT",
   "authors": [
      {
         "name": "DevZair",
         "email": "contact@devzair.fr"
      }
   ],
   "require": {
      "php": ">=7.4",
      "symfony/mime": "^5.4",
      "symfony/dependency-injection": "^5.4",
      "symfony/config": "^5.4",
      "symfony/http-kernel": "^5.4",
      "symfony/validator": "^5.4",
      "doctrine/orm": "^2.10",
      "symfony/mailer": "5.4.45"
   },
   "autoload": {
      "psr-4": {
         "DevZair\\MailLoggerBundle\\": "src/"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "DevZair\\MailLoggerBundle\\Tests\\": "tests/"
      }
   },
   "extra": {
      "branch-alias": {
         "dev-main": "1.0-dev"
      }
   },
   "require-dev": {
      "phpunit/phpunit": "^9.5"
   }
}
